%nav{:role => "navigation"}
  .nav-wrapper.container
    = link_to root_path, class: 'logo-container brand-logo' do
      .logo
        = image_tag('logo.svg')
    %ul.right.hide-on-med-and-down
      = render 'layouts/nav_links'
    %ul#nav-mobile.side-nav
      = render 'layouts/nav_links'
    %a.button-collapse{"data-activates" => "nav-mobile", :href => "#"}
      %i.fa.fa-bars
